<h1>Customer Name: <%= @customer.name %></h1>
<section id="customer_items">
<h2>Items</h2>
<% @customer.items.each do |item| %>
  <section id= "item-<%= item.id %>">
  <p><%= "Item: #{item.name}" %></p>
  <p><%= "Price: $#{item.price}" %></p>
  <p><%= "Supermarket Name: #{item.supermarket.name}" %></p>
  </section> 
<% end %>
</section>